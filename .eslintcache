ser] = useState(null);\n  const [products, setProducts] = useState(null);\n\n  useEffect(async () => {\n    if (!user) {\n      const response = await api.getUserById(currentUser?.email);\n      setUser(response.data.data);\n    } else {\n      setProducts(user.favorites);\n      console.log(user);\n    }\n  }, [user, products]);\n\n  return (\n    <Page\n      loading={loading}\n      title={title}\n      color={\"#fdeded\"}\n      setTitle={setTitle}\n      add={false}\n      FAB=\"none\"\n      dots={false}\n    >\n      <h1>My Favorites</h1>\n      {products?.map((product) => {\n        return (\n          <>\n            <Card style={{ width: \"18rem\" }}>\n              <Card.Img variant=\"top\" src={product[\"photo\"]} />\n              <Card.Body>\n                <Card.Title>{product[\"name\"]}</Card.Title>\n                <Card.Text>{product[\"description\"]}</Card.Text>\n                <Card.Text>{product[\"condition\"]}</Card.Text>\n                <Card.Text>{product[\"address\"]}</Card.Text>\n                <Card.Text>{product[\"price\"] + \"₪\"}</Card.Text>\n              </Card.Body>\n            </Card>\n          </>\n        );\n      })}\n\n      {setActive(false)}\n    </Page>\n  );\n}\n","/Users/jacobsonpro/GitHub/ReBuy/ReBuySite/src/Components/Pages/Settings/ProfileSettings.js",["174","175","176","177","178","179","180","181","182","183","184"],"/Users/jacobsonpro/GitHub/ReBuy/ReBuySite/src/Components/SSO/SignUp.js",["185"],"/Users/jacobsonpro/GitHub/ReBuy/ReBuySite/src/Components/Pages/Home/Products.js",["186","187","188","189","190","191","192","193","194","195","196","197","198","199","200","201","202","203","204","205","206","207"],"/Users/jacobsonpro/GitHub/ReBuy/ReBuySite/src/Components/Utils/Page.js",["208"],"import React from \"react\";\nimport { useIntl } from \"react-intl\";\nimport PageBody from \"./PageBody\";\nimport LoadingOverlay from \"react-loading-overlay\";\nimport icon from \"../../Assets/Images/ReBuyLogoTrans.png\";\n\n