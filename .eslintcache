d) ||\n                    Admins(currentUser.email) ? (\n                      <>\n                        <Button size=\"small\" color=\"primary\">\n                          <DeleteProduct\n                            id={product[\"seconderyId\"]}\n                            deleteHandler={deleteHandler}\n                          />\n                        </Button>\n                        {console.log(product)}\n                        <Button size=\"small\" color=\"primary\">\n                          <UpdateProduct id={product[\"seconderyId\"]} />\n                        </Button>\n                      </>\n                    ) : user &&\n                      user.favorites &&\n                      user.favorites?.some((p) => {\n                        return product.name === p.name;\n                      }) ? (\n                      <Button size=\"small\" color=\"primary\">\n                        <Star\n                          onClick={() => {\n                            Alert(\"Products removed from favorites!\", true);\n                            api.removeFromFavorites(user, product);\n                            setChange((prev) => !prev);\n                          }}\n                        />\n                      </Button>\n                    ) : (\n                      <Button size=\"small\" color=\"primary\">\n                        <StarBorderIcon\n                          onClick={() => {\n                            api.addToFavorites(user, product);\n                            Alert(\"Products added from favorites!\", true);\n                            setChange((prev) => !prev);\n                          }}\n                        />\n                      </Button>\n                    )}\n                  </CardActions>\n                  {/* {console.log(user)} */}\n                </Card>\n                <GridListTileBar\n                  title={\n                    <h6\n                      style={{\n                        font: \"16px bold\",\n                        marginRight: \"5rem\",\n                        color: \"black\",\n                      }}\n                    >\n                      {product[\"name\"].toUpperCase()}\n                    </h6>\n                  }\n                  subtitle={\n                    <>\n                      <h8 style={{ marginRight: \"5rem\", color: \"black\" }}>\n                        {product[\"price\"]}₪\n                      </h8>\n                    </>\n                  }\n                  actionIcon={\n                    <CardActions\n                      style={{ justifyContent: \"center\", height: \"2rem\" }}\n                    >\n                      {(user && user.uid === product.ownerId) ||\n                      Admins(currentUser.email) ? (\n                        <>\n                          <Button size=\"small\" color=\"primary\">\n                            <DeleteProduct\n                              id={product[\"seconderyId\"]}\n                              deleteHandler={deleteHandler}\n                            />\n                          </Button>\n                          <Button size=\"small\" color=\"primary\">\n                            <UpdateProduct id={product[\"seconderyId\"]} />\n                          </Button>\n                        </>\n                      ) : user &&\n                        user.favorites &&\n                        user.favorites?.some((p) => {\n                          return product.name === p.name;\n                        }) ? (\n                        <Button size=\"small\" color=\"primary\">\n                          <Star\n                            onClick={() => {\n                              api.removeFromFavorites(user, product);\n                              setChange((prev) => !prev);\n                            }}\n                          />\n                        </Button>\n                      ) : (\n                        <Button size=\"small\" color=\"primary\">\n                          <StarBorderIcon\n                            onClick={() => {\n                              api.addToFavorites(user, product);\n                              setChange((prev) => !prev);\n                            }}\n                          />\n                        </Button>\n                      )}\n                    </CardActions>\n                  }\n                  className={classes.tileBar}\n                />\n              </GridListTile>\n            ))}\n        </GridList>\n        <GridList className={classes.gridList} cols={3}></GridList>\n      </MediaQuery>\n      <MediaQuery minWidth={800}>\n        {products &&\n          products.map((product, i) => (\n            <Card\n              style={{\n                margin: \"5px\",\n                width: 300,\n                border: \"1px solid #ececec\",\n                borderRadius: \"15px\",\n              }}\n            >\n              {product[\"photo\"] ? (\n                <Carousel>\n                  {product[\"photo\"]\n                    .toString()\n                    .split(\",\")\n                    .map((Image) => (\n                      <Carousel.Item\n                        style={{\n                          width: \"100%\",\n                          height: 200,\n                        }}\n                      >\n                        <img\n                          style={{ width: \"100%\", height: \"100%\" }}\n                          src={Image}\n                          alt={Image}\n                        />\n                      </Carousel.Item>\n                    ))}\n                </Carousel>\n              ) : (\n                <img\n                  style={{ width: \"100%\", height: 200 }}\n                  src={rebuyProduct}\n                  alt={\"rebuyProduct\"}\n                />\n              )}{\" \"}\n              <CardActionArea\n   