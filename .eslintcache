r);\n    }\n  }, [user, products]);\n\n  return (\n    <Page\n      loading={loading}\n      title={title}\n      color={\"#fdeded\"}\n      setTitle={setTitle}\n      add={false}\n      FAB=\"none\"\n      dots={false}\n    >\n      <h1>My Favorites</h1>\n      {products?.map((product) => {\n        return (\n          <>\n            <Card style={{ width: \"18rem\" }}>\n              <Card.Img variant=\"top\" src={product[\"photo\"]} />\n              <Card.Body>\n                <Card.Title>{product[\"name\"]}</Card.Title>\n                <Card.Text>{product[\"description\"]}</Card.Text>\n                <Card.Text>{product[\"condition\"]}</Card.Text>\n                <Card.Text>{product[\"address\"]}</Card.Text>\n                <Card.Text>{product[\"price\"] + \"₪\"}</Card.Text>\n              </Card.Body>\n            </Card>\n          </>\n        );\n      })}\n\n      {setActive(false)}\n    </Page>\n  );\n}\n","/Users/jacobsonpro/GitHub/ReBuy/ReBuySite/src/Components/Pages/Settings/ProfileSettings.js",["187","188","189","190","191","192","193","194","195","196","197"],"import React, { Component, useState, useContext, useEffect } from \"react\";\nimport { AuthContext, Admins } from \"../../SSO/Auth\";\nimport { useIntl } from \"react-intl\";\nimport MediaQuery from \"react-responsive\";\nimport { Form, Card, Dropdown } from \"react-bootstrap\";\nimport { FaPencilAlt } from \"react-icons/fa\";\nimport Person from \"@material-ui/icons/PersonRounded\";\nimport { Avatar } from \"@material-ui/core\";\nimport api from \"../../../API/API\";\nimport CardList from \"../Home/CardList\";\nimport apis from \"../../../API/API\";\n\nexport default class ProfileSetting extends Component {\n  constructor(props) {\n    super(props);\n    this.state = {\n      products: [],\n      images: [],\n      users: [],\n      columns: [],\n      isLoading: this.props.loading,\n      imagePath: \"\",\n      setLoading: this.props.setLoading,\n    };\n  }\n\n  componentDidMount = asy